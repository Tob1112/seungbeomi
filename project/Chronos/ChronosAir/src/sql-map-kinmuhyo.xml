<sqlMap>

	<!-- =================================== -->
	<!-- CREATE SQL							 -->
	<!-- =================================== -->
	<sql id="kinmuhyo.createKinmuhyo">
		CREATE TABLE kinmuhyo (
			nengetsu			TEXT	PRIMARY KEY,
			shinseiBango		TEXT,
			shinseiKubun		TEXT,
			shinseiJokyo		TEXT
		)
	</sql>

	<!-- =================================== -->
	<!-- SELECT SQL							 -->
	<!-- =================================== -->
	<sql id="kinmuhyo.findKinmuhyoList">
		SELECT * FROM kinmuhyo
		ORDER BY nengetsu DESC
	</sql>


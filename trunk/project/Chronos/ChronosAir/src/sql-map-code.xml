<sqlMap>

	<!-- =================================== -->
	<!-- CREATE SQL							 -->
	<!-- =================================== -->
	<sql id="code.createCodeHyo">
		CREATE TABLE codeHyo (
			code 		TEXT,
			value 		TEXT
		)
	</sql>

	<!-- =================================== -->
	<!-- SELECT SQL							 -->
	<!-- =================================== -->
	<sql id="code.findCode">
		SELECT * FROM codeHyo
		ORDER BY code ASC;
	</sql>


CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE SEQUENCE USER_SEQ AS INTEGER START WITH 3 
CREATE SEQUENCE ADDRESS_SEQ AS INTEGER START WITH 4 
CREATE MEMORY TABLE DEPT(DEPT_NO VARCHAR(20) NOT NULL PRIMARY KEY,DEPT_NAME VARCHAR(50))
CREATE MEMORY TABLE EMP(EMP_NO VARCHAR(20) NOT NULL PRIMARY KEY,EMP_NAME VARCHAR(50),DEPT_NO VARCHAR(20),CONSTRAINT FK_DEPT_NO FOREIGN KEY(DEPT_NO) REFERENCES DEPT(DEPT_NO) ON DELETE CASCADE)
CREATE MEMORY TABLE EMP_DETAIL(EMP_NO VARCHAR(20),EMAIL VARCHAR(50),CONSTRAINT FK_EMP_NO FOREIGN KEY(EMP_NO) REFERENCES EMP(EMP_NO) ON DELETE CASCADE)
CREATE USER SA PASSWORD ""
GRANT DBA TO SA

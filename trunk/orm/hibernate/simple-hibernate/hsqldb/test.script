CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE SEQUENCE SEQ_ARTICLE AS INTEGER START WITH 1 
CREATE SEQUENCE SEQ_ATTACHMENT AS INTEGER START WITH 1 
CREATE MEMORY TABLE ARTICLE(ID INTEGER NOT NULL,TITLE VARCHAR(255) NOT NULL,CONTENT LONGVARCHAR NOT NULL,CONSTRAINT PK_ARTICLE PRIMARY KEY(ID))
CREATE MEMORY TABLE ATTACHMENT(ID INTEGER NOT NULL,ARTICLEID INTEGER NOT NULL,FILENAME VARCHAR(255) NOT NULL,CONSTRAINT PK_ATTACHMENT PRIMARY KEY(ID),CONSTRAINT FK_ATTACHMENT FOREIGN KEY(ARTICLEID) REFERENCES ARTICLE(ID))
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 60
